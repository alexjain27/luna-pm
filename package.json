{
  "name": "luna-pm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "prisma": "npx -p node@22 node node_modules/.bin/prisma",
    "db:generate": "npx -p node@22 node node_modules/.bin/prisma generate",
    "db:migrate": "npx -p node@22 node node_modules/.bin/prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx -p node@22 node node_modules/.bin/prisma migrate reset --force --skip-seed && npm run db:seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.2",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.13",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
